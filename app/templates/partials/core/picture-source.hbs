{{#ifUseWebp}}
  <source {{#if media}}media="({{media}})" {{/if}}
          data-srcset="../assets/images/{{img}}.webp, ../assets/images/{{img}}@2x.webp 2x"
          type="image/webp">
{{/ifUseWebp}}

<source {{#if media}}media="({{media}})" {{/if}}
        data-srcset="../assets/images/{{img}}.{{fallback-ext}},
          ../assets/images/{{img}}@2x.{{fallback-ext}} 2x"
  {{#ifCond fallback-ext 'jpg'}} type="image/jpeg" {{else}} type="image/png" {{/ifCond}}>
